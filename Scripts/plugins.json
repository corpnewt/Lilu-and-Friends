          "args": [
            "-p", 
            "./build/Release/VirtualSMC/Tools"
          ], 
          "name": "Create Tools Folder"
        },
        {
          "path": "mv", 
          "args": [
            "./build/Release/package/Kexts/*", 
            "./build/Release/VirtualSMC/Kexts/"
          ], 
          "name": "Move Kexts"
        },
        {
          "path": "mv", 
          "args": [
            "./build/Release/package/Drivers/*", 
            "./build/Release/VirtualSMC/Drivers/"
          ], 
          "name": "Move Drivers"
        },
        {
          "path": "mv", 
          "args": [
            "./build/Release/package/Tools/*", 
            "./build/Release/VirtualSMC/Tools/"
          ], 
